; Engine configuration file.
; It's best edited using the editor UI and not directly,
; unless you know what you are doing.

config_version=5

[application]
config/name="Carrier Landing"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "Compatibility", "Mobile")
boot_splash/show_image=false

[display]
window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[rendering]
renderer/rendering_method="mobile"
renderer/anti_aliasing/quality/msaa_2d=1
renderer/anti_aliasing/quality/screen_space_aa=true

[autoload]
GameManager="*res://scripts/systems/game_manager.gd"

[input]
# Input actions are configured by GameManager._ensure_input_map() at runtime.
